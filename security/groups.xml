<odoo>
    <record model="ir.module.category" id="library.category">
        <field name="name">Library</field>
    </record>

    <record model="res.groups" id="library.group_user">
        <field name="name">Library user</field>
        <field name="category_id" ref="library.category"/>
        <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
    </record>

    <record model="res.groups" id="library.group_manager">
        <field name="name">Library manager</field>
        <field name="category_id" ref="library.category"/>
        <field name="implied_ids" eval="[(4, ref('library.group_user'))]"/>
        <field name="users" eval="[(4, ref('base.user_root')), (4, ref('base.user_admin'))]"/>
    </record>
</odoo>